doctype html
head
    include ../includes/head
    +head
    title= project.name + " | Project 9927 Project"
body
    include ../includes/navbar
    +navbar-include(user)
    h1=project.name
        if teamProject !== undefined
            span
                | (
                a(href=`/team/${teamProject._id}`)=teamProject.name
                |)
    div(class="members")
        ul(id="membersList")
            each val in members
                li=val.username

    div(class="todo")
    div(class="calendar")